{
	"servers": {
		"context7": {
			"args": ["-y", "@upstash/context7-mcp@latest"],
			"command": "npx",
			"type": "stdio"
		},
		"deepwiki": {
			"type": "http",
			"url": "https://api.deepwiki.com/mcp/sse"
		},
		"fetch": {
			"args": ["mcp-server-fetch"],
			"command": "uvx",
			"type": "stdio"
		},
		"figma": {
			"url": "http://127.0.0.1:3845/sse",
			"type": "http"
		},
		"filesystem": {
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"${workspaceFolder}"
			],
			"command": "npx",
			"type": "stdio"
		},
		"git": {
			"args": ["mcp-server-git"],
			"command": "uvx",
			"type": "stdio"
		},
		"github": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/"
		},
		"markitdown": {
			"type": "stdio",
			"command": "uvx",
			"args": ["markitdown-mcp"],
			"gallery": true
		},
		"mcp-server-chart": {
			"args": ["-y", "@antv/mcp-server-chart"],
			"command": "npx",
			"type": "stdio"
		},
		"memory": {
			"args": ["-y", "@modelcontextprotocol/server-memory"],
			"command": "npx",
			"type": "stdio"
		},
		"netlify": {
			"args": ["-y", "@netlify/mcp"],
			"command": "npx",
			"type": "stdio"
		},
		"playwright": {
			"args": ["@playwright/mcp@latest"],
			"command": "npx",
			"type": "stdio"
		},
		"sequentialthinking": {
			"args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
			"command": "npx",
			"type": "stdio"
		},
		"time": {
			"args": ["mcp-server-time", "--local-timezone", "Asia/Tokyo"],
			"command": "uvx",
			"type": "stdio"
		}
	},
	"inputs": []
}
