	"accessibility.signals.terminalBell": {
		"sound": "on"
	},
	"accessibility.verbosity.terminal": false,
	"calva.prettyPrintingOptions": {
		"enabled": true,
		"maxLength": 50,
		"printEngine": "pprint",
		"width": 120
	},
	"claudeCodeChat.thinking.intensity": "ultrathink",
	"claudeCodeChat.wsl.claudePath": "/usr/local/bin/claude",
	"claudeCodeChat.wsl.distro": "Ubuntu",
	"claudeCodeChat.wsl.enabled": false,
	"claudeCodeChat.wsl.nodePath": "/usr/bin/node",
	"coderabbit.agentType": "Claude Code",
	"diffEditor.codeLens": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"diffEditor.maxComputationTime": 0,
	"docker.extension.dockerEngineAvailabilityPrompt": false,
	"editor.accessibilitySupport": "off",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit",
		"source.fixAll.shellcheck": "explicit"
	},
	"editor.defaultFormatter": "biomejs.biome",
	"editor.fontFamily": "PlemolJP, Consolas, 'Courier New', monospace",
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.largeFileOptimizations": false,
	"editor.tabSize": 2,
	"editor.tokenColorCustomizations": {
		"[Tokyo Night]": {
			"textMateRules": [
				{
					"scope": [
						"comment",
						"meta.var.expr storage.type",
						"keyword.control.flow",
						"keyword.control.return",
						"meta.directive.vue punctuation.separator.key-value.html",
						"meta.directive.vue entity.other.attribute-name.html",
						"tag.decorator.js entity.name.tag.js",
						"tag.decorator.js punctuation.definition.tag.js",
						"storage.modifier"
					],
					"settings": {
						"fontStyle": ""
					}
				},
				{
					"scope": [
						"comment keyword.codetag.notation",
						"comment.block.documentation keyword",
						"comment.block.documentation storage.type.class"
					],
					"settings": {
						"foreground": "#bb9af7"
					}
				},
				{
					"scope": ["comment.block.documentation entity.name.type.instance"],
					"settings": {
						"foreground": "#73daca"
					}
				},
				{
					"scope": [
						"comment.block.documentation entity.name.type punctuation.definition.bracket"
					],
					"settings": {
						"foreground": "#bb9af7"
					}
				},
				{
					"scope": ["comment.block.documentation variable"],
					"settings": {
						"foreground": "#e0af68"
					}
				}
			]
		}
	},
	"editor.unicodeHighlight.ambiguousCharacters": true,
	"editor.unicodeHighlight.nonBasicASCII": false,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"files.associations": {
		"*.lock": "jsonc"
	},
	"files.autoGuessEncoding": true,
	"files.encoding": "utf8",
	"files.eol": "\n",
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"git.autofetch": true,
	"git.blame.editorDecoration.enabled": true,
	"git.branchProtection": ["main", "master"],
	"git.confirmSync": false,
	"git.defaultCloneDirectory": "D:\\projects\\",
	"git.diagnosticsCommitHook.enabled": true,
	"git.enableCommitSigning": true,
	"git.enableSmartCommit": true,
	"git.openRepositoryInParentFolders": "never",
	"git.terminalGitEditor": true,
